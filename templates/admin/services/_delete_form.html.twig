<form method="post" action="{{ path('services_delete', {'id': service.id}) }}" onsubmit="return confirm('Voulez vous supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service.id) }}">
    <button class="btn btn-danger"><i class="bi bi-trash3"></i> Supprimer</button>
</form>
